#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/bin/node_modules:/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/node_modules:/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules:/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/bin/node_modules:/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules/esprima/node_modules:/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/esprima@1.2.2/node_modules:/home/zuf/Projects/giustizia_plataform/giustizia-platform/giustizia-platform-complete/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esparse.js" "$@"
else
  exec node  "$basedir/../../bin/esparse.js" "$@"
fi
